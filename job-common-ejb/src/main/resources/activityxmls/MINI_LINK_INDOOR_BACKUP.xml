<?xml version="1.0" encoding="UTF-8"?>
<ActivityInformation platformType="MINI_LINK_INDOOR" jobType="BACKUP" xmlns="com:ericsson:oss:services:shm:job:activity"
                     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:schemaLocation="com:ericsson:oss:services:shm:job:activity activity_information.xsd ">

    <NeActivityInformation neType="DEFAULT">
